def get_count(input_str)
  #your code here
  count = 0
input_str.each_char do |x|
# ВАЖНО использование each_char в связи с тем, что используется строка не переведенная в массив!
  if x == "a" or x==  "e" or x==  "i" or x==  "o" or x==  "u"
    count = count +1
    else
 count = count
   end
end
return count
end
